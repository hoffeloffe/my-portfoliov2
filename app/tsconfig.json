{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
}
